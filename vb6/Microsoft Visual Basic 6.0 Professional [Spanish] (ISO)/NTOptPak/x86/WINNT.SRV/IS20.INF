[Version]
Signature= "$Windows NT$"

;
; Installation .inf for Index Server 2.0
;
; History: 1/21/97    dlee      Created
;
; Changes from 1.1 => 2.0:
;  +   ixsso.dll copied
;  +   ixsso object self-registered
;  +   IFilters self-register instead of setup code doing it
;  +   cisvc.exe copied
;  +   cisvc service added
;  +   put default catalog in registry
;  +   start cisvc at end of install
;  +   rm old reg params
;  +   add new reg params
;  +   new ixsso samples
;      new idq/htx samples
;      search for all catalogs and put them in the registry?
;

;
; This list of optional components is presented to the user
; for selection
;
[Optional Components]
indexsrv
indexsrv_system
indexsrv_language
indexsrv_samples
indexsrv_onlinedoc
indexsrv_language_english_us
indexsrv_language_english_uk
indexsrv_language_french_french
indexsrv_language_german_german
indexsrv_language_italian_italian
indexsrv_language_swedish_default
indexsrv_language_spanish_modern
indexsrv_language_dutch_dutch
indexsrv_language_japanese_default

[indexsrv]
OptionDesc = %indexsrvOptionDesc%
Tip = %indexsrvTip%
IconIndex = 28
ISVersionMajor = 2
ISVersionMinor = 0

[indexsrv_system]
OptionDesc = %indexsrv_systemOptionDesc%
Tip = %indexsrv_systemTip%
Parent = indexsrv
CopyFiles = ISSharedSystemFiles, ISFiles, ISAdminHTMFiles, ISAdminIDQFiles, ISSSOFiles, ISPerfFiles
AddReg = ISReg, ISReg.Performance, indexsrv_language_neutral_reg, ISReg.ErrorFiles
IconIndex = 28
MyUninstall = uninstall_indexsrv_system
DelFiles = ISOldUrlFiles, ISFilterDelFiles, ISVer1AdminHTMFiles, ISVer1AdminIDQFiles, ISVer1HelpFiles, ISOldStartMenuFiles
AddService = cisvc, 0, indexsrv_cisvc
Needs = iis_www
Modes=0,1,3

[indexsrv_pdffilter]
CopyFiles = ISPDFFiles

[indexsrv_cisvc]
DisplayName = %cisvcDisplayName%
; share-process so system logon is enforced + interactive
ServiceType = 288
StartType = 2
ErrorControl = 1
ServiceBinary = %11%\cisvc.exe
Dependencies = RPCSS

[uninstall_indexsrv_system]
DelFiles = ISFiles, ISAdminHTMFiles, ISAdminIDQFiles, ISSSOFiles, ISPerfFiles, ISPDFFiles
DelReg = UninstallISReg, UninstallISReg.Performance, uninstall_indexsrv_language_neutral_reg, UninstallISReg.HTMLClsids
DelService = cisvc

[indexsrv_samples]
OptionDesc = %indexsrv_samplesOptionDesc%
Tip = %indexsrv_samplesTip%
Parent = indexsrv
IconIndex = 32
CopyFiles = ISSampleHTMFiles, ISSampleIDQFiles, ISSampleOOPFiles, ISASPSampleFiles
DelFiles  = ISDeleteHtwFiles
MyUninstall = uninstall_indexsrv_samples
Needs = indexsrv_system
Modes=0,1,3

[ISDeleteHtwFiles]
qsumrhit.htw
qfullhit.htw

[uninstall_indexsrv_samples]
DelFiles = ISSampleHTMFiles, ISSampleIDQFiles, ISSampleOOPFiles, ISASPSampleFiles

[indexsrv_onlinedoc]
OptionDesc = %indexsrv_onlinedocOptionDesc%
Tip = %indexsrv_onlinedocTip%
Parent = indexsrv
IconIndex = 49
